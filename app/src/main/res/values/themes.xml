        <item name="colorPrimary">@color/gris</item>
        <item name="colorPrimaryVariant">@color/gris</item>
        <item name="colorOnPrimary">@color/blanco</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/amarillo</item>
